<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ktfit">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".AccountInfo"></activity>
        <activity android:name=".MainActivity" />
        <activity android:name=".UserActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".StartWorkoutActivity"
            android:label="Start Workout" />
        <activity
            android:name=".WaterCaffeineTrackerActivity"
            android:label="Water and Caffeine Tracker" />
        <activity
            android:name=".PlannerActivity"
            android:label="Workout Planner" />
        <activity
            android:name=".MilestonesActivity"
            android:label="Milestones" />
        <activity
            android:name=".HistoryActivity"
            android:label="History Records" />
        <activity
            android:name=".FriendsActivity"
            android:label="Connect With Friends" />
    </application>

</manifest>